{
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "memory": 384,
            "portMappings": [
                {
                    "hostPort": 0,
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "name": "{CONTAINER_NAME}",
            "image": "{ECR_REPOSITORY_DOMAIN}:{DOCKER_IMAGE}",
            "cpu": 0
        }
    ],
    "family": "{33333}"
}
